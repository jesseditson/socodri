{{#each funnels}}
  <div class="panel sc-panel">
      <div class="sc-card-header">
          <div class="sc-card-details">
              <div class="col-md-12">
                  <h2 class="card-heading"><a href="/funnel/{{slug}}/">{{name}}</a></h2>
              </div>
              <div class="col-md-3">
                  <div class="sc-card-label">
                      Total Conversions
                  </div>
                  <div class="sc-card-value">
                      {{formatNumber  insights.conversions }} <small>({{percentOf insights.conversions ../insights.conversions }}%) </small>
                  </div>
              </div>
              <div class="col-md-3">
                  <div class="sc-card-label">
                      Cost Per Conversion
                  </div>
                  <div class="sc-card-value">
                      {{ costPer insights.conversions ../insights.spend }}
                  </div>
              </div>
              <div class="col-md-3">
                  <div class="sc-card-label">
                      Conversion Revenue
                  </div>
                  <div class="sc-card-value">
                      {{ formatMoney insights.conversion_revenue }}
                  </div>
              </div>
              <div class="col-md-3">
                  <div class="sc-card-label">
                      ROAS
                  </div>
                  <div class="sc-card-value">
                      {{ roas insights.spend insights.conversion_revenue }}
                  </div>
              </div>
              <div class="clearfix"></div>
          </div>
      </div>
  </div>
{{/each}}
